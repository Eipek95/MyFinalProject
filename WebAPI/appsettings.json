{
  "TokenOptions": {
    "Audience": "engin@engin.com", //projenin web adresi Audience->kitle
    "Issuer": "engin@engin.com", //Issuer Audience uygulayan
    "AccessTokenExpiration": 10, //token geçerlilik süresi dk cinsinden 10 dk
    "SecurityKey": "mysupersecretkeymysupersecretkey" //tokenı oluştururken asp.net kullanacağı anahtar adı.uzun olması gerekir
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
